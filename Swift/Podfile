# coding: utf-8
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

use_frameworks!
target 'AEPSampleApp' do

  # step-init-start
  pod 'AEPServices'
  pod 'AEPCore'
  pod 'AEPLifecycle'
  pod 'AEPIdentity'
  pod 'AEPSignal'
  pod 'AEPRulesEngine'
  # step-init-end


  # step-extension-start
  pod 'AEPSampleExtensionSwift', :git => 'https://github.com/adobe/aepsdk-sample-extension-ios.git', :branch => 'main'
  # step-extension-end


  # step-edge-start
  pod 'AEPEdge', :git => 'git@github.com:adobe/aepsdk-edge-ios.git', :branch => 'staging'
  pod 'AEPEdgeConsent'
  pod 'AEPEdgeIdentity'
  # step-edge-end

  # step-assurance-start
  pod 'ACPCore', :git => 'https://github.com/adobe/aepsdk-compatibility-ios.git', :branch => 'main'
  pod 'AEPAssurance'
  # step-assurance-end
  
  pod 'AEPUserProfile'

end
